<%- include("partials/header") %>
    <section class="events" id="events-section">
        <div class="content-wrapper">
            <div class="inner-container container-fluid">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <div class="section-heading">
                            <div class="filter-categories">
                                <ul class="project-filter">
                                    <li class="filter" data-filter="all"><span>Transactions</span></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-10 col-sm-12 col-md-offset-1">
                        <div class="projects-holder">
                            <div class="event-list">
                                <ul>
                                    <% for(let transaction of transactions){ %>
                                        <li class="project-item first-child design">
                                            <ul class="event-item design">
                                                <li>
                                                    <div class="date">
                                                        <span>
                                                            <%= Math.floor((Math.random() * 30)) %><br>May
                                                        </span>
                                                    </div>
                                                </li>
                                                <li>
                                                    <h4>
                                                        <%= transaction.fromName %>
                                                    </h4>
                                                </li>
                                                <li>
                                                    <div class="time">
                                                        <span>
                                                            <%= transaction.transfer %><br>Credits
                                                        </span>
                                                    </div>
                                                </li>
                                                <li style="margin-top: 4%;">
                                                    <h4 style="margin-left: 10%;">
                                                        <%= transaction.toName %>
                                                    </h4>
                                                </li>
                                            </ul>
                                        </li>

                                        <% } %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <%- include("partials/footer") %>